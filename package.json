    "vue-analytics": "^5.20.2",
    "vue-draggable-resizable": "^2.1.0",
    "vue-firestore": "^0.3.22",
    "vue-router": "^3.1.3",
    "vuetify": "^1.5.22",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
